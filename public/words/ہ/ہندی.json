  {
    "line": "ہِندی مسلمان",
    "start": 0,
    "end": 5,
    "line_number": 1,
    "file": 406,
    "first": false,
    "other": ""
  },
  {
    "line": "ہِندی اِسلام",
    "start": 0,
    "end": 5,
    "line_number": 1,
    "file": 417,
    "first": false,
    "other": ""
  },
  {
    "line": "کرے یہ کافرِ ہندی بھی جُرأتِ گُفتار",
    "start": 13,
    "end": 17,
    "line_number": 3,
    "file": 454,
    "first": true,
    "other": "اگر نہ ہو اُمَرائے عرب کی بےادبی!"
  },
  {
    "line": "نہ میں اعجمی نہ ہندی، نہ عراقی و حجازی",
    "start": 16,
    "end": 20,
    "line_number": 3,
    "file": 467,
    "first": true,
    "other": "کہ خودی سے مَیں نے سِیکھی دوجہاں سے بےنیازی"
  },
  {
    "line": "ہندی مکتب",
    "start": 0,
    "end": 4,
    "line_number": 1,
    "file": 473,
    "first": false,
    "other": ""
  },
  {
    "line": "خودی کی موت سے ہِندی شکستہ بالوں پر",
    "start": 15,
    "end": 20,
    "line_number": 7,
    "file": 476,
    "first": true,
    "other": "قفَس ہُوا ہے حلال اور آشیانہ حرام!"
  },
  {
    "line": "ہِندی بھی فرنگی کا مقلّد، عجمی بھی!",
    "start": 0,
    "end": 5,
    "line_number": 4,
    "file": 525,
    "first": false,
    "other": "کس درجہ یہاں عام ہُوئی مرگِ تخیّل"
  }
]
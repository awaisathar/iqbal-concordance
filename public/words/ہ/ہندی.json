[
  {
    "line": "ترانۂ ہندی",
    "start": 6,
    "end": 10,
    "line_number": 1,
    "file": 39,
    "first": false,
    "other": ""
  },
  {
    "line": "ہندی ہیں ہم، وطن ہے ہندوستاں ہمارا",
    "start": 0,
    "end": 4,
    "line_number": 19,
    "file": 39,
    "first": false,
    "other": "مذہب نہیں سِکھاتا آپس میں بَیر رکھنا"
  },
  {
    "line": "نغمہ ہندی ہے تو کیا، لَے تو حجازی ہے مری!",
    "start": 5,
    "end": 9,
    "line_number": 218,
    "file": 106,
    "first": false,
    "other": "عجَمی خُم ہے تو کیا، مے تو حجازی ہے مری"
  },
  {
    "line": "بُت ہندی کی محبت میں بَرہمن بھی ہوئے",
    "start": 4,
    "end": 8,
    "line_number": 158,
    "file": 121,
    "first": false,
    "other": "مثلِ انجم اُفُقِ قوم پہ روشن بھی ہوئے"
  },
  {
    "line": "کچھ کیفیتِ مسلمِ ہندی تو بیاں کر",
    "start": 17,
    "end": 21,
    "line_number": 9,
    "file": 150,
    "first": true,
    "other": "واماندۂ منزل ہے کہ مصروفِ تگ و تاز"
  },
  {
    "line": "شکوہِ ترکمانی، ذہنِ ہندی، نُطقِ اعرابی",
    "start": 20,
    "end": 24,
    "line_number": 10,
    "file": 163,
    "first": false,
    "other": "عطا مومن کو پھر درگاہِ حق سے ہونے والا ہے"
  },
  {
    "line": "یہ ہندی، وہ خُراسانی، یہ افغانی، وہ تُورانی",
    "start": 3,
    "end": 7,
    "line_number": 109,
    "file": 163,
    "first": true,
    "other": "تُو اے شرمندۂ ساحل! اُچھل کر بےکراں ہو جا"
  },
  {
    "line": "یا بحث میں اُردو ہندی ہے یا قربانی یا جھٹکا ہے",
    "start": 17,
    "end": 21,
    "line_number": 13,
    "file": 180,
    "first": false,
    "other": "یا باہم پیار کے جلسے تھے، دستورِ محبّت قائم تھا"
  },
  {
    "line": "یہ کافرِ ہندی ہے بےتیغ و سناں خُوں‌ریز",
    "start": 9,
    "end": 13,
    "line_number": 22,
    "file": 233,
    "first": false,
    "other": "یوں دادِ سخن مجھ کو دیتے ہیں عراق و پارس"
  },
  {
    "line": "دیا اقبالؔ نے ہندی مسلمانوں کو سوز اپنا",
    "start": 14,
    "end": 18,
    "line_number": 15,
    "file": 266,
    "first": true,
    "other": "یہ اک مردِ تن‌آساں تھا، تن‌آسانوں کے کام آیا"
  },
  {
    "line": "نفس ہندی، مقامِ نغمہ تازی",
    "start": 4,
    "end": 8,
    "line_number": 4,
    "file": 300,
    "first": false,
    "other": "کوئی دیکھے تو میری نَے نوازی"
  },
  {
    "line": "کافرِ ہندی ہُوں مَیں، دیکھ مرا ذوق و شوق",
    "start": 6,
    "end": 10,
    "line_number": 49,
    "file": 317,
    "first": true,
    "other": "دل میں صلٰوۃ و دُرود، لب پہ صلوٰۃ و دُرود"
  },
  {
